alias reload!='. ~/.zshrc'

# Git aliases
alias lazygit='git add .; git commit -m '...'; git push;'
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '
alias gk='gitk --all&'
alias gx='gitx --all'

# Alternative pager
alias vless='vim -R'
